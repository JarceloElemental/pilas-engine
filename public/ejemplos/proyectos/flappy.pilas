      {
        "nombre": "gallina",
        "codigo": "class gallina extends Actor {\n    propiedades = {\n        x: 0,\n        y: 0,\n        imagen: \"imagenes:gallina/gallina_vuela_3\",\n\n        figura: \"circulo\",\n        figura_radio: 30,\n        figura_sin_rotacion: true,\n        figura_dinamica: true,\n        figura_rebote: 0\n    };\n\n    iniciar() {\n        this.crear_animacion(\n            \"gallina_vuela\", [\n                \"imagenes:gallina/gallina_vuela_1\",\n                \"imagenes:gallina/gallina_vuela_1\",\n                \"imagenes:gallina/gallina_vuela_2\",\n                \"imagenes:gallina/gallina_vuela_3\",\n                \"imagenes:gallina/gallina_vuela_2\"\n            ],\n            15\n        );\n        this.crear_animacion(\n            \"pierde\", [\n                \"imagenes:gallina/gallina_sin_piel\",\n\n            ],\n            15\n        );\n        this.crear_animacion(\"gallina_muere\", [\"imagenes:gallina/gallina_muere\"], 20);\n        this.crear_animacion(\n            \"gallina_sin_piel\", [\"imagenes:gallina/gallina_sin_piel\"],\n            20\n        );\n\n        this.estado = \"vuela\";\n    }\n\n    actualizar() {}\n\n    vuela_iniciar() {\n        this.reproducir_animacion(\"gallina_vuela\");\n    }\n\n    vuela_actualizar() {\n        this.rotacion = this.velocidad_y * 2;\n\n        if (this.pilas.control.arriba) {\n            this.saltar();\n        }\n    }\n\n    saltar() {\n        this.impulsar(0, 7)\n    }\n\n    cuando_comienza_una_colision(actor: Actor) {\n\n        if (actor.tiene_etiqueta(\"moneda\")) {\n            actor.eliminar();\n        } else {\n            this.estado = \"pierde\";\n        }\n    }\n\n    pierde_iniciar() {\n        this.dinamico = false;\n        this.reproducir_animacion(\"pierde\");\n        this.decir(\"¡Ouch!\");\n\n        this.pilas.escena.camara.vibrar(0.2, 0.5);\n    }\n\n    pierde_actualizar() {\n\n    }\n}"
      },
