        "codigo": "class escena1 extends Escena {\n    iniciar() {\n        this.pilas.cada(2, () => {\n            this.crear_moneda();\n        });\n    }\n\n    crear_moneda() {\n        let moneda = this.pilas.actores.moneda();\n        moneda.imagen = \"imagenes:objetos/corazon_completo\"\n        moneda.y = this.pilas.azar(-200, 200);\n        moneda.x = 300;\n\n        moneda.actualizar = function() {\n            this.x -= 5;\n\n            if (this.x < -220) {\n                this.eliminar();\n            }\n        }\n    }\n\n    actualizar() {}\n\n    cuando_hace_click() {\n        let gallina = this.pilas.obtener_actor_por_nombre(\"gallina\");\n        gallina.saltar();\n    }\n}"
