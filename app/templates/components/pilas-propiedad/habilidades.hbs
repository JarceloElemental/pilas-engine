<div class="pb2">

  <div class="dib w4">

    <div class="dib texto">
      {{etiqueta propiedad.propiedad}}
    </div>

  </div>

  <div class="dib texto w104 v-top pointer">
    {{#power-select-multiple
      options=habilidades
      selected=(get objeto propiedad.propiedad)
      placeholder="Ninguna"
      onchange=(action "cuando_selecciona")
      as |habilidad|
    }}
      {{habilidad}}
    {{/power-select-multiple}}
  </div>

</div>
